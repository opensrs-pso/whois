#status
  should: %s == :registered

#available?
  should: %s == false

#registered?
  should: %s == true


#created_on
  should: %s CLASS(time)
  should: %s == Time.parse("2006-11-15")

#updated_on
  should: %s CLASS(time)
  should: %s == Time.parse("2007-04-09")

#expires_on
  should: %s raise_error(Whois::AttributeNotSupported)
