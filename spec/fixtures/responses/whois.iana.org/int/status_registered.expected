  should: %s[1].name == "maxima.nra.nato.int"
  should: %s[1].ipv4 == "193.110.130.68"
  should: %s[2] CLASS(nameserver)
  should: %s[2].name == "ns.namsa.nato.int"
  should: %s[2].ipv4 == "208.161.248.15"
  should: %s[3] CLASS(nameserver)
