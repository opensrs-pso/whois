  should: %s[1] CLASS(nameserver)
  should: %s[1].name == "ns2.qatarbank.com"
  should: %s[1].ipv4 == nil
  should: %s[2] CLASS(nameserver)
  should: %s[2].name == "ns3.qatarbank.com"
  should: %s[2].ipv4 == nil
